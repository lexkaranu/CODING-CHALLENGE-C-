using System;

namespace ArrayNo1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string[] orderIds = { "B123", "C234", "A345", "C15", "B177", "G3003", "C235", "B179" };

            foreach (string item in orderIds)
            {
                if (item.StartsWith("B"))
                {
                    Console.WriteLine("Order ID: " + item);
                }
            }
        }
    }
}