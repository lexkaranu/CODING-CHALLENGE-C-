using System;

namespace Increment
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] A = {1,2,3,4,5};
            HashSet<int> abNumbers = new HashSet<int>();

            foreach (int num in A)
            {
                abNumbers.Add(Math.Abs(num));
            }

             

        }
    }
}