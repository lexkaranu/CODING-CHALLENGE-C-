using System;

namespace Increment
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int value = 1;
            value++;
            Console.WriteLine("First:" + value);
            Console.WriteLine("Second:" + value++);
            Console.WriteLine("Third:" + value);
            Console.WriteLine(" First:" + (++value));

            Console.WriteLine("-----");

            
            ++value;
            value++;
            Console.WriteLine("First:" + value);
        }
    }
}